            "description": " (Constant value: FIND)"
          },
          "iss": {
            "maxLength": 200,
            "minLength": 1,
            "type": "string"
          },
          "userSessionId": {
            "minLength": 1,
            "pattern": "^[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RequestHeaderModel": {
        "required": [
          "iss",
          "userSessionId"
        ],
        "type": "object",
        "properties": {
          "userSessionId": {
            "minLength": 1,
            "pattern": "^[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}$",
            "type": "string"
          },
          "iss": {
            "maxLength": 200,
            "minLength": 1,
            "type": "string"
          },
          "correlationId": {
            "maxLength": 36,
            "minLength": 0,
            "pattern": "^[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}$",
            "type": "string",
