{
  "openapi": "3.0.4",
  "info": {
    "title": "MaPSCDAService, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null",
    "version": "1.0"
  },
  "paths": {
    "/claims-gathering-redirect": {
      "get": {
        "tags": [
          "ClaimsRedirect"
        ],
        "parameters": [
          {
            "name": "request",
            "in": "header",
            "schema": {
              "$ref": "#/components/schemas/RequestHeaderModel"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/rqp": {
      "get": {
        "tags": [
          "MapsCdaService"
        ],
        "parameters": [
          {
            "name": "headerModel",
            "in": "header",
            "schema": {
              "$ref": "#/components/schemas/RequestHeaderModel"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/RqpResponseModel"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RqpResponseModel"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/RqpResponseModel"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/redirect-details": {
      "post": {
        "tags": [
          "MapsCdaService"
        ],
        "parameters": [
          {
            "name": "headerModel",
            "in": "header",
            "schema": {
              "$ref": "#/components/schemas/RequestHeaderModel"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/RedirectResponseModel"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RedirectResponseModel"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/RedirectResponseModel"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              },
              "application/json": {
                "schema": {
                  "type": "string"
                }
              },
              "text/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "RedirectResponseModel": {
        "type": "object",
        "properties": {
          "redirectTargetUrl": {
            "pattern": "^https?:\\/\\/([a-zA-Z0-9\\-]+\\.)+[a-zA-Z]{2,}(:\\d{1,5})?(\\/[^\\s]*)?$",
            "type": "string",
            "nullable": true
          },
          "rqp": {
            "pattern": "^[A-Za-z0-9\\-_]+(\\.[A-Za-z0-9\\-_]+){2}$",
            "type": "string",
            "nullable": true
          },
          "scope": {
            "minLength": 1,
            "type": "string",
            "nullable": true
          },
          "responseType": {
            "enum": [
              "code"
            ],
            "type": "string",
            "description": " (Constant value: code)",
            "nullable": true
          },
          "prompt": {
            "enum": [
              "login"
            ],
            "type": "string",
            "description": " (Constant value: login)",
            "nullable": true
          },
          "service": {
            "minLength": 1,
            "type": "string",
            "nullable": true
          },
          "codeChallengeMethod": {
            "enum": [
              "S256"
            ],
            "type": "string",
            "description": " (Constant value: S256)",
            "nullable": true
          },
          "codeChallenge": {
            "minLength": 1,
            "type": "string",
            "nullable": true
          },
          "codeVerifier": {
            "minLength": 1,
            "type": "string",
            "nullable": true
          },
          "requestId": {
            "pattern": "^[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}$",
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RequestHeaderModel": {
        "required": [
          "iss",
          "userSessionId"
        ],
        "type": "object",
        "properties": {
          "userSessionId": {
            "minLength": 1,
            "pattern": "^[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}$",
            "type": "string"
          },
          "iss": {
            "maxLength": 200,
            "minLength": 1,
            "type": "string"
          },
          "correlationId": {
            "maxLength": 36,
            "minLength": 0,
            "pattern": "^[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}$",
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RqpResponseModel": {
        "type": "object",
        "properties": {
          "rqp": {
            "pattern": "^[A-Za-z0-9\\-_]+(\\.[A-Za-z0-9\\-_]+){2}$",
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}