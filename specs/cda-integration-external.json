                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/jwk_uri": {
            "get": {
                "tags": [
                    "JwkUri"
                ],
                "summary": "/jwk_uri - GET",
                "operationId": "get-jwk_uri",
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/peis/{peis_id}": {
            "get": {
                "tags": [
                    "Peis"
                ],
                "summary": "/peis/{peis_id} - GET",
